<h1>Listing alunos</h1>

<table>
  <tr>
    <th align="left">Nome</th>
    <th align="left">Fone</th>
    <th align="left">Nascimento</th>
    <th align="left">Batismo</th>
    <th align="left">Email</th>
    <th align="left">Endereco</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @alunos.each do |aluno| %>
  <tr>
    <td><%= aluno.Nome %></td>
    <td><%= aluno.Fone %></td>
    <td><%= aluno.Nascimento %></td>
    <td><%= aluno.Batismo %></td>
    <td><%= aluno.Email %></td>
    <td><%= aluno.Endereco %></td>
    <td><%= link_to 'Show', aluno %></td>
    <td><%= link_to 'Edit', edit_aluno_path(aluno) %></td>
    <td><%= link_to 'Destroy', aluno, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Aluno', new_aluno_path %>
